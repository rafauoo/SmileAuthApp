{
  "expo": {
    "name": "SmileTrainApp",
    "slug": "SmileTrainApp",
    "version": "1.3.0",
    "orientation": "portrait",
    "scheme": "SmileTrainApp",
    "icon": "./src/assets/images/smiley_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/smiley_entry.gif",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/smiley_logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/images/smiley_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-video",
      "expo-font"
    ]
  }
}
